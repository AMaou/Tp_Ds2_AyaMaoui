<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="{{ app.request.basepath }}/lib/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="{{ app.request.basepath }}/css/mymovie.css" rel="stylesheet">
    <title>Mymovies - {% block title %}{% endblock %}</title>
</head>
<body>
    <div class="container">
        <nav class="navbar navbar-default navbar-fixed-top navbar-inverse" role="navigation">
            <div class="container">
                
                <div class="collapse navbar-collapse" id="navbar-collapse-target">

                    <ul class="nav navbar-nav ">
                        <li>
                            <a href="" class="dropdown-toggle" data-toggle="dropdown">MyMovies <b class="caret"></b></a>

                        </li>
                        
                        <li>
                            <a href="" class="dropdown-toggle" data-toggle="dropdown">Ajouter un film <b class="caret"></b></a>
                                                      
                        </li> 
    
                    </ul>
                </div>
            </div><!-- /.container -->
        </nav>
        {% autoescape %}
        <div id="content">{% block content %}{% endblock %}</div>
        {% endautoescape %}
    </div>

    <!-- jQuery -->
    <script src="{{ app.request.basepath }}/lib/jquery/jquery.min.js"></script>
    <!-- JavaScript Boostrap plugin -->
    <script src="{{ app.request.basepath }}/lib/bootstrap/js/bootstrap.min.js"></script>
</body>
</html>